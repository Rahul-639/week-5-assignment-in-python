class StackNode:
    def __init__(self, data: int):
        self.data = data
        self.next = None
class MyStack:
    def __init__(self):
        self.top = None
    def push(self, x: int) -> None:
        new_node = StackNode(x)
        new_node.next = self.top
        self.top = new_node
    def pop(self) -> int:
        if not self.top:
            return -1
        val = self.top.data
        self.top = self.top.next
        return val
